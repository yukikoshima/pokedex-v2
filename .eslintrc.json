{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/vue3-essential",
    "plugin:@typescript-eslint/recommended",
    "@nuxtjs/eslint-config-typescript", // 追加
    "prettier" // 追加
  ],
  "overrides": [],
  "parser": "vue-eslint-parser", // 追加
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "parser": "@typescript-eslint/parser" // 追加
  },
  "plugins": ["vue", "@typescript-eslint"],
  "rules": {}
}
